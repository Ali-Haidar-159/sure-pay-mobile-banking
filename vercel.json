{
  "version": 2,
  "builds": [{ "src": "**/*", "use": "@vercel/static" }],
  "routes": [
    { "src": "/", "dest": "/View/index.html" },
    { "src": "/(.+\\.html)", "dest": "/View/$1" },
    { "src": "/public/CSS/(.*)", "dest": "/CSS/$1" },
    { "src": "/public/JS/(.*)", "dest": "/JS/$1" },
    { "src": "/public/images/(.*)", "dest": "/images/$1" }
  ]
}

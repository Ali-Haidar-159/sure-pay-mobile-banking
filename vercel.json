{
  "version": 2,
  "builds": [{ "src": "**/*", "use": "@vercel/static" }],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/", "dest": "/View/index.html" },
    { "src": "/index.html", "dest": "/View/index.html" },
    { "src": "/public/(.*)", "dest": "/public/$1" },
    { "src": "/(.*)\\.html", "dest": "/View/$1.html" }
  ]
}
